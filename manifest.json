{
    "manifest_version": 2,
    "name": "Screen Time",
    "version": "1.0.0",
    "description": "Screen Time: Social Networking Sites",
    "icons": {
        "16":"images/16x16.png",
        "32":"images/32x32.png",
        "48":"images/48x48.png",
        "128":"images/128x128.png"
    },
    "background":{
        "scripts":["background.js"],
        "persistent":false
    },
   "options_page":"options.html",
    "browser_action": {
        "default_icon":"icon.png",
        "default_popup":"pop_up.html"
    },
    "permissions":["tabs","https://www.google.com/*"]
}